# vim: filetype=sh
alias h="cd ~/ && ls -a" \
d="cd ~/Documents && ls -a" \
D="cd ~/Downloads && ls -a" \
m="cd ~/Music && ls -a" \
pp="cd ~/Pictures && ls -a" \
vv="cd ~/Videos && ls -a" \
cf="cd ~/.config && ls -a" \
sc="cd ~/.local/bin && ls -a" \
mn="cd /mnt && ls -a" \
cfa="$EDITOR ~/.config/aliasrc" \
cfs="$EDITOR ~/.config/shortcutrc" \
cfz="$EDITOR ~/.zshrc" \
cfk="$EDITOR ~/.config/yabai/shkrc" \
